{
   "bugs": {
      "url": "https://github.com/F-O-T/montte-nx/issues"
   },
   "dependencies": {
      "superjson": "2.2.2",
      "zod": "4.0.14"
   },
   "description": "Official TypeScript SDK to interact with the ContentaGen API. Provides lightweight client methods for listing and retrieving content, automatic input validation, date parsing, and robust error handling.",
   "devDependencies": {
      "@biomejs/biome": "2.3.10",
      "@types/bun": "1.3.5",
      "bumpp": "10.3.2",
      "bunup": "0.16.11",
      "typescript": "5.9.3"
   },
   "exports": {
      ".": {
         "bun": "./src/index.ts",
         "import": {
            "default": "./dist/index.js",
            "types": "./dist/index.d.ts"
         },
         "types": "./src/index.ts"
      },
      "./posthog": {
         "bun": "./src/posthog.ts",
         "import": {
            "default": "./dist/posthog.js",
            "types": "./dist/posthog.d.ts"
         },
         "types": "./src/posthog.ts"
      },
      "./package.json": "./package.json"
   },
   "files": [
      "dist"
   ],
   "homepage": "https://github.com/F-O-T/montte-nx/blob/master/libraries/sdk",
   "license": "Apache-2.0",
   "module": "./dist/index.js",
   "name": "@f-o-t/contentta-sdk",
   "peerDependencies": {
      "typescript": ">=4.5.0"
   },
   "peerDependenciesMeta": {
      "typescript": {
         "optional": true
      }
   },
   "private": false,
   "publishConfig": {
      "access": "public"
   },
   "repository": {
      "type": "git",
      "url": "https://github.com/F-O-T/montte-nx.git"
   },
   "scripts": {
      "build": "bunup",
      "check": "biome check --write .",
      "dev": "bunup --watch",
      "release": "bumpp --commit --push --tag",
      "test": "bun test",
      "test:watch": "bun test --watch",
      "typecheck": "tsc"
   },
   "type": "module",
   "types": "./dist/index.d.ts",
   "version": "0.16.1"
}
